/* components/unlink/unlink.wxss */
@import "../../app.wxss";
.unlink{
  position: fixed;
  z-index: 99;
  display: flex;
  height: 100vh;
  width: 100%;
  background: #fff;
  padding-top: 50px;
  flex-direction: column;
  align-items: center;
  text-align: center;
}
.unlink .image{
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
}
.unlink .text{
  font-size: 30rpx;
  color: #999;
  margin: 10px auto 30px;
}
.unlink .kj-button{
  color: #fff;
}
.unlink .button{
 
  height: 60rpx;
  font-size: 26rpx;
  margin-bottom: 20px;
  line-height: 60rpx;
}
.unlink .contact{
   border: 1px solid #f5f6f7
}